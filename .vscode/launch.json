    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Android Manifest Read",
            "program": "${workspaceFolder}/tasks/AndroidManifestRead/task.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "INPUT_SOURCEPATH": "${workspaceFolder}/tasks/AndroidManifestRead/Tests/AndroidManifest-01.xml"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Git Last Commit Date",
            "program": "${workspaceFolder}/tasks/GitLastCommitDate/task.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}